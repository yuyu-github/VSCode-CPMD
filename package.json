{
    "name": "cpmd-format",
    "displayName": "CPMD Format",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "cpmd",
            "aliases": ["CPMD", "cpmd"],
            "extensions": [".cpmd"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "cpmd",
            "scopeName": "source.cpmd",
            "path": "./syntaxes/cpmd.tmLanguage.json"
        }],
        "snippets": [{
            "language": "cpmd",
            "scopeName": "source.cpmd",
            "path": "./snippet/cpmd.snippet.json"
        }]
    }
}
